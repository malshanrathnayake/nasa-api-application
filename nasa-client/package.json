    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "axios": "^1.6.8",
    "bootstrap": "4.6.2",
    "classnames": "2.3.2",
    "headroom.js": "0.12.0",
    "moment": "2.29.4",
    "nouislider": "15.4.0",
    "react": ">=16.0.0",
    "react-datetime": "3.2.0",
    "react-dom": ">=16.0.0",
    "react-router-dom": "6.11.1",
    "react-scripts": "5.0.1",
    "react-toastify": "^10.0.5",
    "reactstrap": "8.10.0",
    "sass": "1.62.1"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "devDependencies": {
    "@types/markerclustererplus": "2.1.33",
